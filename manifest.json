{
    "manifest_version": 3,
    "name": "NeoExamShield",
    "version": "1.3",
    "description": "To prevent malpractice, identifies and blocks third-party browser extensions during tests on the Iamneo portal.",
    "key": "",
    "permissions": [
        "activeTab",
        "clipboardRead",
        "clipboardWrite",
        "management",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "commands": {
        "search-mcq": {
            "suggested_key": {
                "default": "Alt+X"
            },
            "description": "Solve MCQs"
        },
        "custom-copy": {
            "suggested_key": {
                "default": "Alt+C"
            },
            "description": "Custom Copy"
        },
        "custom-paste": {
            "suggested_key": {
                "default": "Alt+V"
            },
            "description": "Custom Paste"
        },
        "search-openai": {
            "suggested_key": {
                "default": "Alt+Z"
            },
            "description": "Search OpenAI"
        }
    }
}
